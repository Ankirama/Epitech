CC	= gcc

RM	= rm -f

CFLAGS	= -W -Wall -Wextra
CFLAGS	+= -I.

NAME	= my_boogle

SRCS	= main.c \
	  my_check_options.c \
	  fun.c \
	  my_size.c \
	  my_grid.c \
	  my_word.c \
	  my_color.c \
	  my_strcmp.c \
	  nbr.c \
	  algo.c

OBJS	= $(SRCS:.c=.o)

all:	$(NAME)

$(NAME): $(OBJS)
	$(CC) -o $(NAME) $(OBJS)

clean:
	$(RM) $(OBJS)

fclean: clean
	$(RM) $(NAME)

re: fclean all

.PHONY: all clean fclean re
